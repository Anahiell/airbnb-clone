{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "ReverseProxy": {
    "Routes": {
      "product-swagger": {
        "ClusterId": "productCluster",
        "Match": {
          "Path": "/swagger/product/{**catchall}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/swagger/product" }
        ]
      }
    },
    "Clusters": {
      "productCluster": {
        "Destinations": {
          "product": {
            "Address": "http://172.27.0.4:8080/"
          }
        }
      }
    }
  }
}
